{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "AutocleanEEG",
  "description": "Professional EEG Software",
  "background": {
    "color": {
      "dark": "#222831",
      "light": "#EEEEEE"
    },
  "decoration": "windows"
  },
  "colors": {
    "primary": "#6d45dc",
    "light": "#9f8ded",
    "dark": "#6a42d7"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "welcome",
              "installation",
              "index",
              "quickstart",
                "essentials/cli-introduction",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "customizations/cli-themes",
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.autocleaneeg.org",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://github.com/cincibrainlab/autocleaneeg_pipeline/discussions",
          "icon": "slack"
        },
        {
          "anchor": "Issues",
          "href": "https://github.com/cincibrainlab/autocleaneeg_pipeline/issues",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "primary": {
      "href": "https://github.com/cincibrainlab/autoclean_pipeline",
      "type": "github"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/cincibrainlab/autoclean_pipeline",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
